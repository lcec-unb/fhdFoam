// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beta_m		1.2e-03;	// Pyromagnetic coefficient
chi0		3.5;		// Magnetic susceptibility (EFH3 TDS)
Hmax		1.0e+5;		// Maximum applied field
mag_height	5e-02;		// Magnet height
