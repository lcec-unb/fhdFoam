FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      magneticProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beta_m		1.2e-03;	// Pyromagnetic coefficient
chi0		3.52;		// Magnetic susceptibility (EFH3 TDS)
Hmax		1.0e+7;		// Maximum applied field
mag_height	5e-02;		// Magnet height

HModel      linear;

linearCoeffs
{
    C0    ($Hmax 0.0 0.0);
}

