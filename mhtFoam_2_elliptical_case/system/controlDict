functions
{
	probes
	{
		type		probes;
		libs		("libsampling.so");
		writeControl	timeStep;
		writeInterval	1;
		
		fields
		(
			T
			W
		);
		
		probeLocations
		(
			(7.13381544E-03 0.045 0)
			(8.28661919E-02 0.045 0)
		);
	}
}
